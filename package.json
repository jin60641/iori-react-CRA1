    "socket.io-client": "^2.0.4"
  },
  "scripts": {
    "development": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "node ./server/app.js",
    "api": "NODE_ENV=development PORT=3333 node ./server/app.js",
    "sass": "node-sass-chokidar -w src -o src",
    "test:react": "react-scripts test --env=jsdom",
    "test:server": "npm run eslint && npm run eslint:test && cross-env NODE_ENV=test PORT=7654 mocha -r ./test/test_helper.js --recursive ./test",
    "test:chrome": "cross-env NODE_ENV=test mocha --inspect-brk test",
    "eslint": "eslint ./server/app.js",
    "eslint:test": "eslint ./test"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "chai-deep-match": "^1.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "mocha": "^5.2.0",
    "node-sass-chokidar": "^1.3.3",
    "nodemon": "^1.12.1",
    "supertest": "^3.1.0"
  },
  "proxy": {
    "/socket.io": {
      "target": "http://localhost:3333",
      "ws": true
    },
    "/api": {
      "target": "http://localhost:3333"
    },
    "/public": {
      "target": "http://localhost:3333"
    },
    "/files": {
      "target": "http://localhost:3333"
    }
  },
  "engines": {
    "node": ">= 10.1.0"
  }
