{
  "name": "iori",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "breakpoint-sass": "^2.7.1",
    "classnames": "^2.2.5",
    "core-js": "^2.5.7",
    "include-media": "^1.4.9",
    "react": "^16.1.1",
    "react-click-outside": "^3.0.1",
    "react-cookie": "^2.1.4",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-textarea-autosize": "^7.0.4",
