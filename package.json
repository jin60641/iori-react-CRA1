{
  "name": "iori",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "breakpoint-sass": "^2.7.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "core-js": "^2.5.7",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "include-media": "^1.4.9",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-click-outside": "^3.0.1",
    "react-cookie": "^2.1.4",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.2.1",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2",
    "sass-loader": "^7.1.0",
    "socket.io-client": "^2.0.4",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "development": "node scripts/start.js",
    "build": "node scripts/build.js",
    "start": "node ./server/app.js",
    "api": "NODE_ENV=development PORT=3333 node ./server/app.js",
    "test:react": "node scripts/test.js --env=jsdom",
    "test:server": "npm run eslint && npm run eslint:test && cross-env NODE_ENV=test PORT=7654 mocha -r ./test/test_helper.js --recursive ./test",
    "test:chrome": "cross-env NODE_ENV=test mocha --inspect-brk test",
    "eslint:react": "eslint ./src",
    "eslint:server": "eslint ./server/app.js",
    "eslint:test": "eslint ./test"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-arrays": "^2.0.0",
    "chai-deep-match": "^1.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.12.1",
    "supertest": "^3.1.0"
  },
  "proxy": {
    "/socket.io": {
      "target": "http://localhost:3333",
      "ws": true
    },
    "/api": {
      "target": "http://localhost:3333"
    },
    "/public": {
      "target": "http://localhost:3333"
    },
    "/files": {
      "target": "http://localhost:3333"
    }
  },
  "engines": {
    "node": ">= 10.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
